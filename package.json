{
  "name": "jupyter_mandalab",
  "version": "0.1.0",
  "description": "Mandalab visualization in Jupyter Lab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/ElectroDevTeam/jupyter_mandalab",
  "bugs": {
    "url": "https://github.com/ElectroDevTeam/jupyter_mandalab/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Stopel",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElectroDevTeam/jupyter_mandalab.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.2",
    "@jupyterlab/apputils": "^2.0.2",
    "@jupyterlab/docmanager": "^2.0.2",
    "@lumino/widgets": "^1.11.1",
    "@types/react-dom": "^16.9.5",
    "@types/react-highlight-words": "^0.16.1",
    "axios": "^0.19.2",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-highlight-words": "^0.16.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
